<% [16, 32, 48, 64, 96].each do |size| %>
  <link href=<%= "/favicon-#{size}x#{size}.png" %> rel="icon" type="image/png" sizes=<%= "#{size}x#{size}" %> >
<% end %>

<% [128, 192].each do |size| %>
  <link href=<%= "/android-icon-#{size}x#{size}.png" %> rel="icon" type="image/png" sizes=<%= "#{size}x#{size}" %> >
<% end %>

<% [57, 60, 72, 76, 114, 120, 144, 152, 180].each do |size| %>
  <link href=<%= "/apple-icon-#{size}x#{size}.png" %> rel="apple-touch-icon" type="image/png" sizes=<%= "#{size}x#{size}" %> >
<% end %>
